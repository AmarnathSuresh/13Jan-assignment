#include<iostream>
#include<string.h>
using namespace std;

class programming
{
	private:
		char text[100];
	public:
		programming()
		{
			cin.getline(text,100);
		}
char get_text()
{
	if (strlen(text)!=0)
	{
		cout<<"I love 	"<<text<<endl;
	}
	else
	{
		cout<<"I love programming languages";
	}

}
};
int main()
{
	programming textbox;
	cout<<textbox.get_text();
}
